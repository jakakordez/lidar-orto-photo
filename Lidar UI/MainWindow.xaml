<Window x:Class="Lidar_UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lidar_UI"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="800">
    <Grid>
        <local:mapView x:Name="mapView" Margin="415,59,10,10"/>
        <Button x:Name="btnStart" Content="Start" HorizontalAlignment="Left" Height="23" Margin="495,30,0,0" VerticalAlignment="Top" Width="75" Click="btnStart_Click"/>
        <CheckBox x:Name="chkNormals" Content="Include normals" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtPath" HorizontalAlignment="Left" Height="23" Margin="10,30,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="400"/>
        <ListBox x:Name="lstOutput" HorizontalAlignment="Left" Margin="10,58,0,10" Width="400"/>
        <Button x:Name="btnFolder" Content="..." HorizontalAlignment="Left" Margin="415,30,0,0" VerticalAlignment="Top" Width="75" Height="23" Click="btnFolder_Click"/>

    </Grid>
</Window>
